
<%  @divId    	 = 'container' + graph.id.to_s %>
<%  @color_y1 	 =  graph.color_y1  %>
<%  @color_y2 	 =  graph.color_y2 %>
<%  @title    	 =  [graph.name] %>
<%  @sp_graph_id =  @selected_category.id %>

<% @names        = [] %>
<% @graph_types   = [] %>
<% @colors       = [] %>
<% @ids		     = [] %>

<% adts = graph.adts.order('order_num asc') %>
<% adts.each do |a| %>
	<%#= debugger %>
	<%  @names.push(a.y_legend) %>
    <%  @colors.push(a.color_y) %>
    <%  @ids.push(a.id) %>
    <%  @graph_types.push(a.graph_type?) %>
<% end %>



<%= render :partial => "charts"  %>