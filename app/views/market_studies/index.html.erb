<% content_for :head do %>
  <%= stylesheet_link_tag  'commentaries', media: 'all' %>
<% end %>


<div class="row">


  <div class="col-sm-8 blog-main">
    <%= render :partial => @market_studies %>

  <center><%= paginate @market_studies %></center>

  </div><!-- /.blog-main -->

  <div class="col-sm-3 col-sm-offset-1 blog-sidebar">
    
    <div class="sidebar-module sidebar-module-inset">
      <div class="panel panel-default">
      <div class="panel-heading"><%= current_user.present? ?  "Your Info":  "Sign In" %></div>
        <div class="panel-body">
          <% if current_user.blank? %>
          <%= render :partial => 'devise/sessions/form'%>
          <% else %>
            <strong>Signed in as : </strong><br>
            <p><%= current_user.email %></p>
          <% end %>
        </div>
      </div>
    </div>

    <div>
      <a class="twitter-timeline" href="https://twitter.com/IndextResearch" data-chrome="nofooter">Tweets by IndextResearch</a> <script async src="//platform.twitter.com/widgets.js" charset="utf-8"></script>
    </div>
    
    <!-- <div class="sidebar-module">
      <h4>Elsewhere</h4>
      <ol class="list-unstyled">
        <li><a href="#">GitHub</a></li>
        <li><a href="#">Twitter</a></li>
        <li><a href="#">Facebook</a></li>
      </ol>
    </div> -->
  </div><!-- /.blog-sidebar -->
</div><!-- /.row -->


